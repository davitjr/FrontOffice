<form style="max-width:550px" name="StatmentByEmailOrderForm" ng-controller="StatmentByEmailOrderCtrl" ng-init="getStatmentByEmailOrder(selectedOrderId);getStatementFrequency()">
    <loading-control class="col-sm-12" scopevariable="frequency"></loading-control>
    <div ng-if="order.Quality !=31 && order.Quality !=30 && order.Quality !=32 && order.Quality !=40 && order.Quality !=41 && order.Quality !=6 && order.Quality !=50" id="statmentByemailOrder_menu" class="menu-bar dialog-menu light-green " style="margin-left:-5px;margin-top:0">
        <ul id="menu" class="dialog-menu">

            <li class="root-menu-item-5 " ng-if="order.Quality !=31 && order.Quality !=30 && order.Quality !=32 && order.Quality !=40 && order.Quality !=41 && order.Quality !=6 && order.Quality !=50" ng-controller="PopUpCtrl" ng-click="params={removableorder:order};openWindow('/RemovalOrder/RemovalOrder','Հրաժարման հայտ','removalOrder',callbackgetStatmentByEmailOrder)">
                <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon closeAccountButton">Հրաժարման հայտ</span>
            </li>

        </ul>
    </div>

    <div class="product-details bhoechie-tab-content" style="max-width:500px;padding-left:10px">

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Հայտի համար</div>
            <div class="product-detail-cell product-detail-value">{{order.Id}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable"><span style="white-space: nowrap">Մուտքագրման ա/թ</span></div>
            <div class="product-detail-cell product-detail-value">{{order.RegistrationDateString}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գործառնական օր</div>
            <div class="product-detail-cell product-detail-value">{{order.OperationDateString}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Էլեկտրոնային հասցեներ</div>
            <div class="product-detail-cell product-detail-value">{{order.MainEmail}}<span ng-if="order.SecondaryEmail!=''">, {{order.SecondaryEmail}}</span></div>
        </div>


        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Հաշվեհամարները</div>
            <div class="product-detail-cell product-detail-value"><span id="span">{{accounts}}</span></div>
        </div>

        <div ng-if="order.Fees" class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Պարբերականություն</div>
            <div class="product-detail-cell product-detail-value">{{frequency[order.Periodicity]}}</div>
        </div>


        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Կարգավիճակ</div>
            <div ng-if="order.Quality==30" class="product-detail-cell product-detail-value greenStyle">{{order.QualityDescription}}</div>
            <div ng-if="order.Quality==31" class="product-detail-cell product-detail-value redStyle">{{order.QualityDescription}}</div>
            <div ng-if="order.Quality!=31 && order.Quality!=30" class="product-detail-cell product-detail-value">{{order.QualityDescription}}</div>
        </div>

        <div ng-if="order.Quality==31" class="">
            <div ng-controller="OrderCtrl" ng-init="getOrderRejectHistory(selectedOrderId)" class="" style="clear:both;color:#6F6A6A;font-weight:bold">Մերժման պատճառ`</div>
            <div class="redStyle">&nbsp;&nbsp;&nbsp;{{ rejectHistory.ReasonDescription }}</div>
        </div>
    </div>
    <div style="clear:both;color:#6F6A6A;font-weight:bold;margin-left:10px">Կատարվել է {{selectedSourceDescription}}-ի միջոցով</div>

</form>
    



