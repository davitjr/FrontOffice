                <ul>
                    <li class="root-menu-item-5 " ng-if="creditline.Type!=9 && creditline.Quality!=10" ng-controller="PopUpCtrl" ng-click="params={productId:creditline.ProductId};openWindow('/Overdue/OverdueDetails','Ժ/անց պատմություն')" data-permission-selector=""><span id="h-menu-item-5">Ժ/անց պատմություն</span></li>
                    <li class="root-menu-item-6 " ng-if="creditline.Type!=9 && creditline.Quality!=10" ng-controller="PopUpCtrl" ng-click="params={productId:creditline.ProductId};openWindow('/Loan/LoanProductProlongations','Վարկային գծերի երկարացման դիմումներ')" data-permission-selector=""><span id="h-menu-item-6">Երկարաձգման դիմումներ</span></li>
                    <li class="root-menu-item-6 " ng-controller="CreditLineProlongationOrderCtrl" ng-click="saveCreditLineProlongationOrder(productid)"><span id="h-menu-item-6">Վարկային գծի երկարաձգում</span></li>

                    <li class="root-menu-item-3" ng-class="{'my_disable':disabelButton==true}" ng-controller="PopUpCtrl" ng-if="creditline.Quality<=11 && creditline.Quality!=10 && creditline.Quality!=40 && isOnlineAcc==true" ng-click="params={productId:creditline.ProductId,actionType:2,includingSurcharge:1,productType:creditline.ProductType};openWindow('/ClassifiedLoanActionOrder/PersonalClassifiedLoanActionOrder','Վարկային գծի դուրսգրում','newClassifiedLoanActionOrder')">
                        <span id="h-menu-item-3"> Դուրս գրել</span>
                    </li>
                    <li class="root-menu-item-3 " ng-class="{'my_disable':disabelButton==true}" ng-controller="PopUpCtrl" ng-if="creditline.Quality==11 && isOnlineAcc==true" ng-click="params={productId:creditline.ProductId,actionType:1,includingSurcharge:1,productType:creditline.ProductType};openWindow('/ClassifiedLoanActionOrder/PersonalClassifiedLoanActionOrder','Վարկային գծի հետբերում','newClassifiedLoanActionOrder')">
                        <span id="h-menu-item-3"> Հետ բերել </span>
                    </li>
                    <li class="root-menu-item-3 " ng-class="{'my_disable':disabelButton==true}" ng-controller="PopUpCtrl" ng-if="creditline.Quality==11 && isOnlineAcc==true" ng-click="params={productId:creditline.ProductId,actionType:3,includingSurcharge:1,productType:creditline.ProductType};openWindow('/ClassifiedLoanActionOrder/PersonalClassifiedLoanActionOrder','Արտաբալանսից հանում','newClassifiedLoanActionOrder')">
                        <span id="h-menu-item-3"> Հանել արտաբալանսից </span>
                    </li>
                    <li id="v-menuItem-3.2" ng-if="$root.SessionProperties.IsChiefAcc == true && ((creditline.Quality==0 || creditline.Quality==1) || ( creditline.Quality==11 && creditline.OverdueLoanDate == null ) )" ng-controller="PopUpCtrl" ng-click="params={productid:creditline.ProductId,currentCreditline:creditline,isCreditLine:true};openWindow('/LoanDelayOrder/LoanDelayOrder','Վարկային գծի հետաձգում','loanDelayOrder')">
                        <span>Վարկային գծի հետաձգում</span>
                    </li>
                </ul>
            </li>

            <li ng-if="($root.SessionProperties.AdvancedOptions['isOnlineAccChief']=='1' || $root.SessionProperties.AdvancedOptions['isOnlineAcc']=='1'  ) &&   creditline.Type==18 || creditline.Type==46 || creditline.Type==9 || creditline.Type==36 || creditline.Type==25 " class="root-menu-item-3 " ng-controller="PopUpCtrl" ng-click="params={productId:creditline.ProductId,loanType:creditline.Type==9 ? 9: ((creditline.Type==36 || creditline.Type==25) ? 25:18)};openWindow('/CreditCommitmentForgivenessOrder/Index','Ներում','newMature')">
