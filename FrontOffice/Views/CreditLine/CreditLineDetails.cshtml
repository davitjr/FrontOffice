                    <li id="v-menuItem-5.1" class="" ng-click="saveCreditLineTerminationOrder(productid)" data-permission-selector="">
                        <span>Դադարեցնել</span>
                    </li>
                    <li id="v-menuItem-5.2" class="last" ng-if="creditline.Type !=25 && creditline.Type !=18" ng-click="getCreditLineTerminationApplication()" data-permission-selector="">
