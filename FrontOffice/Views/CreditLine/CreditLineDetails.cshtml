            </li>

        </ul>
        <ul ng-controller="LoanProductActivationOrderCtrl" id="menu" class="dialog-menu light-green" ng-if="(creditline.Type==30 || creditline.Type==54 || creditline.Type==50 || creditline.Type==51  )&& (creditline.Type!=9 || (creditline.Type==9 && canmature == true )) ">
