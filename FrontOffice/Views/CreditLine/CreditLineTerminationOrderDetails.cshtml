
    <form style="width:500px" ng-controller="CreditLineCtrl" ng-init="getCreditLineTerminationOrder(selectedOrderId);IsCreditLineActivateOnApiGate(selectedOrderId)">
        <loading-control style="margin-left:20em" scopevariable="order"></loading-control>
        <div ng-if="order.Quality!=undefined && order.Quality !=31 && order.Quality !=30 && order.Quality !=32 && order.Quality !=40 && order.Quality !=41 && order.Quality !=6 && order.Quality !=50 && (((order.Quality == 3 || order.Quality == 20) && $root.SessionProperties.IsCalledFromHB == true) || $root.SessionProperties.IsCalledFromHB != true)" id="creditLineTerminationOrderDetails_menu" class="menu-bar dialog-menu light-green " style="margin-left:0;margin-top:0">            
            <ul id="menu" class="dialog-menu">
                <li class="root-menu-item-5 " ng-if="order.Quality !=31 && order.Quality !=30 && order.Quality !=32 && order.Quality !=40 && order.Quality !=41 && order.Quality !=6 && order.Quality !=50 && $root.SessionProperties.IsCalledFromHB != true" type="button" ng-controller="PopUpCtrl" ng-click="params={removableorder:order};openWindow('RemovalOrder/RemovalOrder','Հրաժարման հայտ','removalOrder',callbackgetCreditLineTerminationOrder)">
                    <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon closeAccountButton">Հրաժարման հայտ</span>
                </li>
                <li class="root-menu-item-3 " ng-if="order.Quality!=undefined && (order.Quality == 3 || order.Quality == 20) && $root.SessionProperties.IsCalledFromHB == true && $root.SessionProperties.AdvancedOptions.isOnlineAcc=='1'" ng-controller="OrderCtrl" ng-click="confirmOrder(order.Id);">
                    <span id="h-menu-item-3"><img id="h-menu-icon-3" class="h-menu-icon" src="~/Content/Images/confirm.png" style="height: 30px;width:36px;margin-top: 6px;">Կատարել</span>
                </li>
            </ul>
        </div> 
        <div class="product-details bhoechie-tab-content" style="min-width:500px;margin-right:10px">
            <div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Հայտի համար</div>
                <div class="product-detail-cell product-detail-value">{{order.Id}}</div>
            </div>

            <div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Մուտքագրման ա/թ</div>
                <div class="product-detail-cell product-detail-value">{{order.RegistrationDateString}}</div>
            </div>

            <div class="product-detail-row" ng-if="$root.SessionProperties.IsCalledFromHB != true">
                <div class="product-detail-cell product-detail-lable">Գործառնական օր</div>
                <div class="product-detail-cell product-detail-value">{{order.OperationDateString}}</div>

            </div>


            <div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Վարկային գծի տեսակ </div>
                <div class="product-detail-cell product-detail-value">{{closedCreditLine.TypeDescription}}</div>

            </div>

            
            <div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Քարտի համար</div>
                <div class="product-detail-cell product-detail-value">{{closedCreditLine.CardNumber}}</div>
            </div>

            <div ng-if="isApiGate==0" class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Դադարեցման եղանակ</div>
                <div class="product-detail-cell product-detail-value">APIGate</div>
            </div>

            <div ng-if="isApiGate==1" class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Դադարեցման եղանակ</div>
                <div class="product-detail-cell product-detail-value">ARCA ֆայլով</div>
            </div>

            
            <div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Պայմանագրային գումար</div>
                <div class="product-detail-cell product-detail-value">{{closedCreditLine.StartCapital}} {{creditline.Currency}} </div>

            </div>

            <div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Պայմանագրային գումար</div>
                <div class="product-detail-cell product-detail-value">{{closedCreditLine.StartCapital}} {{creditline.Currency}} </div>

            </div>


            <div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Տոկոսադրույք</div>
                <div class="product-detail-cell product-detail-value">{{closedCreditLine.InterestRate*100 | number:2}} % </div>

            </div>

            <div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Կարգավիճակ</div>
                <div ng-if="order.Quality==30" class="product-detail-cell product-detail-value greenStyle">{{order.QualityDescription}}</div>
                <div ng-if="order.Quality==31" class="product-detail-cell product-detail-value redStyle">{{order.QualityDescription}}</div>
                <div ng-if="order.Quality!=31 && order.Quality!=30" class="product-detail-cell product-detail-value">{{order.QualityDescription}}</div>
            </div>

            <div ng-if="order.Quality==31">
                <div ng-controller="OrderCtrl" ng-init="getOrderRejectHistory(selectedOrderId)" class="" style="clear:both;color:#6F6A6A;font-weight:bold">Մերժման պատճառ`</div>
                <div class="redStyle">&nbsp;&nbsp;&nbsp;{{ rejectHistory.ReasonDescription }}</div>
            </div>

        </div>

        <div style="clear:both;color:#6F6A6A;font-weight:bold;margin-left:10px" ng-if="$root.SessionProperties.IsCalledFromHB != true">Կատարվել է {{selectedSourceDescription}}-ի միջոցով</div>

</form>




