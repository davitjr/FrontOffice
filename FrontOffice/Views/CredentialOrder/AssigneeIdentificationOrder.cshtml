@{
    Layout = null;
}

<div id="AssigneeIdentificationOrderLoad" class="overlay hidden">
    <img class="loader-image non-selectable" src="../../Content/Images/invisible.png" />
</div>
<form ng-controller="CredentialCtrl" style="min-width:650px;" ng-init="initAssigneeIdentificationOrder()" id="AssigneeIdentificationOrderForm" name="AssigneeIdentificationOrderForm" role="form" novalidate class="form-horizontal" prevent-enter-submit>

    <div id="assigneeIdentificationOrder_menu" class="menu-bar dialog-menu light-green">
        <ul id="menu" class="dialog-menu">
            <li class="root-menu-item-4 " ng-click="AssigneeIdentificationOrderForm.$setSubmitted();AssigneeIdentificationOrderForm.$valid && saveAssigneeIdentificationOrder();">
                <span id="h-menu-item-4"><img id="h-menu-icon-4" class="submitButton h-menu-icon">Պահպանել</span>
            </li>
            <li class="root-menu-item-5 " onclick="CloseBPDialog('assigneeIdentificationOrder')">
                <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon cancelButton">Հրաժարվել</span>
            </li>
        </ul>
        <div id="right-menu" style="display:none;">
        </div>
    </div>
    <div>
        <div class="col-sm-12">
            <div style="max-width:520px;min-width:520px;">
                <errors></errors>
            </div>
            <div class=" form-group">
                <label class="tab_title col-sm-5 " for="RegistrationDate">Մուտքագրման ա/թ</label>
                <div class="col-sm-6 row">
                    <div class="multiple_columns col-sm-8 ">
                        <input disabled="disabled" type="date" class="form-control tab_title unstyled" style="text-align: left;" id="RegistrationDate" name="RegistrationDate" ng-model="order.OperationDate" required>
                    </div>
                </div>
            </div>
       
                
 
                    <div>
                        <select data-ng-model="noncashopperson" ng-if="$root.SessionProperties.CustomerType!=6 && (ordertype == 51 || ordertype == 95)" class="form-control " name="Persons" id="Persons" style="max-width:100%;"
                                data-ng-options="a.CustomerNumber+' '+a.PersonName+' '+a.PersonLastName+' '+a.PersonDocument  for a in persons" ng-change="getCustomer(noncashopperson.CustomerNumber)">
                            <option value="" hidden>Ընտրեք գործարք կատարողին...</option>
                        </select>
                    </div>
                    <br />
                    <div class="form-group " >
                        <label class="tab_title col-sm-5" for="number">Հաճախորդ</label>
                        <div class="col-sm-6 row">
                            <div class="row-no-margin input-group">
                                <input type="text" required onkeypress='return event.charCode >= 48 && event.charCode <= 57' class="form-control tab_title" id="number" ng-minlength="0 || 12" maxlength="12" ng-change="changeCustomerNumber()" ng-enter="order.Credential.AssigneeList[0].CustomerNumber.length == 12 && getCustomer(order.Credential.AssigneeList[0].CustomerNumber)" name="number" data-ng-model="order.Credential.AssigneeList[0].CustomerNumber">
                                <div class="input-group-btn" style="width:auto">
                                    <button type="button" ng-disabled="order.Credential.AssigneeList[0].CustomerNumber.length!=12" class="btn btn-primary" ng-click="addCustomer(customerNumber)" title="Լրացնել տվյալները"><span class="glyphicon glyphicon-plus"></span></button>
                                    <button type="button" class="btn btn-primary searchButton" ng-click="searchCustomers()" title="Հաճախորդի որոնում"><span class="glyphicon glyphicon-search greenStyle"></span></button>

                                </div>
                            </div>
                            <ng-messages ng-if="AssigneeIdentificationOrderForm.$submitted || AssigneeIdentificationOrderForm.number.$dirty"
                                         for="AssigneeIdentificationOrderForm.number.$error">
                                <ng-message when="minlength" class="errors">
                                    Մուտքագրեք ամբողջ համարը(12 նիշ)
                                </ng-message>

                                <ng-message when="required" class="errors">
                                   Հաճախորդը մուտքագրված չէ։
                                </ng-message>
                            </ng-messages>
                        </div>
                    </div>

                    <div class="form-group ">
                        <label class="tab_title col-sm-5" for="amount"> Անուն</label>
                        <div class="col-sm-6 row">
                            <div class="row-no-margin">
                                <input disabled="disabled" type="text" class="form-control tab_title" id="Name" data-ng-model="FirstName" onkeypress="return isArmenianUnicode(event.keyCode)" onpaste="return CheckPasteText(event)" name="Name" style="text-align:left" required>
                            </div>
                            <div class="row-no-margin">
                                <ng-messages ng-if="formname.$submitted || formname.Name.$dirty"
                                             for="formname.Name.$error">
                                    <ng-message when="required" class="errors">
                                        Անունը լրացված չէ
                                    </ng-message>
                                </ng-messages>
                            </div>

                        </div>
                    </div>

                    <div class="form-group ">
                        <label class="tab_title col-sm-5" for="amount"> Ազգանուն</label>
                        <div class="col-sm-6 row">
                            <div class="row-no-margin">
                                <input disabled="disabled" type="text" class="form-control tab_title" id="LastName" data-ng-model="LastName" onkeypress="return isArmenianUnicode(event.keyCode)" onpaste="return CheckPasteText(event)" name="LastName"  style="text-align:left"  required>
                            </div>
                            <div class="row-no-margin">
                                <ng-messages ng-if="formname.$submitted || formname.LastName.$dirty"
                                             for="formname.LastName.$error">
                                    <ng-message when="required" class="errors">
                                        Ազգանունը լրացված չէ
                                    </ng-message>
                                </ng-messages>
                            </div>

                        </div>
                    </div>


                 

                    <div  class="form-group ">
                        <label class="tab_title col-sm-5" for="amount"> Անձնագիր</label>
                        <div class="col-sm-6 row">
                            <div class="row-no-margin">
                                <input disabled="disabled" type="text" class="form-control tab_title" required  id="Passport" name="Passport" style="text-align:left" data-ng-model="Passport">
                            </div>
                            <div class="row-no-margin">
                                <ng-messages ng-if="formname.$submitted || formname.Passport.$dirty"
                                             for="formname.Passport.$error">
                                    <ng-message when="required" class="errors">
                                        Անձնագիրը լրացված չէ
                                    </ng-message>
                                </ng-messages>
                            </div>
                        </div>
                    </div>

                

                    <div ng-show="warnings.length>0" class="panel panel-warning">
                        <div class="panel-heading"><span class="glyphicon glyphicon-warning-sign"></span> Ուշադրություն</div>
                        <div class="panel-body">
                            <div ng-repeat="war in warnings">
                                {{$index+1}}.{{::war}}
                            </div>
                        </div>
                    </div>
     



        </div>
    </div>



</form>











