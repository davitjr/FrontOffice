                <div class="product-detail-cell product-detail-value">{{order.OPPerson.PersonNoSocialNumber}}</div>
            </div>

            <div ng-if="order.OPPerson.CustomerNumber!='' && $root.SessionProperties.IsCalledFromHB != true" class="product-detail-row">
