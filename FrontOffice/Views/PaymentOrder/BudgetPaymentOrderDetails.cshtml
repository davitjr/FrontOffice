                <div class="product-detail-cell product-detail-value">{{order.OPPerson.PersonDocument}}</div>
            </div>

            <div ng-if="order.OPPerson.PersonSocialNumber!='' && order.OPPerson.PersonSocialNumber!=undefined && $root.SessionProperties.IsCalledFromHB != true" class="product-detail-row">
