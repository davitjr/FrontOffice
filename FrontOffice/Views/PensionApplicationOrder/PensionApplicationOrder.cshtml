@{
    Layout = null;
}
<style type="text/css">
    #accountOrderForm .ui-widget input {
        font-family: 'webly_semibold';
    }
</style>
<div id="pensionAppliactionLoad" class="overlay hidden">
    <img class="loader-image non-selectable" src="../../Content/Images/invisible.png" />
</div>
<form ng-controller="PensionApplicationOrderCtrl" name="PensionApplicationOrderForm" id="PensionApplicationOrderForm" role="form" style="min-width:650px;" novalidate class="form-horizontal" labels-required>
    <div id="newaccount_menu" class="menu-bar dialog-menu light-green">
        <ul id="menu" class="dialog-menu">
            <li class="root-menu-item-3 " ng-controller="PensionApplicationCtrl" ng-if="order.Type != 94 && order.PensionApplication.Account.AccountNumber!=undefined" ng-click="accountNoticeForm(order.PensionApplication.Account.AccountNumber)">
                <span id="h-menu-item-3"><img id="h-menu-icon-3" class="h-menu-icon printButton">Հաշվի ծանուցման թերթիկ</span>
            </li>
            <li class="root-menu-item-3 " ng-controller="PensionApplicationCtrl" ng-if="order.Type == 94" ng-click="pensionAgreement()">
                <span id="h-menu-item-3"><img id="h-menu-icon-3" class="h-menu-icon printButton">Վերագրանցման դիմում</span>
            </li>
            <li class="root-menu-item-4 " ng-click="PensionApplicationOrderForm.$setSubmitted();PensionApplicationOrderForm.$valid && savePensionApplicationOrder()">
                <span id="h-menu-item-4"><img id="h-menu-icon-4" class="submitButton h-menu-icon">Պահպանել</span>
            </li>
            <li class="root-menu-item-5 " onclick="CloseBPDialog('newPensionAppliactionOrder')">
                <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon cancelButton">Հրաժարվել</span>
            </li>
        </ul>
        <div id="right-menu" style="display:none;">
        </div>
    </div>

    <div style="max-width:530px">
        <errors></errors>
    </div>

    <div class="col-sm-10">





        <div class=" form-group">
            <label class="tab_title col-sm-5 " for="RegistrationDate">Մուտքագրման ա/թ</label>
            <div class="col-sm-6 row">
                <div class="multiple_columns col-sm-8 ">
                    <input disabled="disabled" type="date" class="form-control tab_title unstyled" style="text-align: left;" id="RegistrationDate" name="RegistrationDate" ng-model="order.OperationDate" required>
                </div>
            </div>
        </div>

        <div class=" form-group" ng-if="false">
            <label class="tab_title col-sm-5 " for="ContractNumber">Դիմումի համար</label>
            <div class="col-sm-6 row">
                <div class="multiple_columns col-sm-8 ">
                    <input type="number" class="form-control tab_title unstyled" disabled="disabled" style="text-align: left;" id="ContractNumber" name="ContractNumber" ng-model="order.PensionApplication.ContractNumber">
                </div>
            </div>
        </div>



        <div class="form-group" ng-init="getPensionAppliactionQualityTypes()" ng-if="false">
            <label class="tab_title col-sm-5" for="Quality">Համաձայնագրի կարգավիճակ</label>
            <div class="col-sm-6">
                <div class="row">
                    <select ng-show="qualityTypes" data-ng-model="order.PensionApplication.Quality" disabled="disabled" name="Quality" id="Quality" class="form-control"
                            data-ng-options="k as v for (k,v) in qualityTypes">
                        <option value="" hidden="hidden" style="margin-left:25px">Ընտրեք...</option>
                    </select>
                </div>
                <div class="row">
                    <ng-messages ng-if="PensionApplicationOrderForm.$submitted || PensionApplicationOrderForm.Quality.$dirty"
                                 for="PensionApplicationOrderForm.Quality.$error">
                        <ng-message when="required" class="errors">
                            Կարգավիճակը ընտրված չէ
                        </ng-message>
                    </ng-messages>
                </div>
            </div>
        </div>




        <div class="form-group" ng-init="getPensionAppliactionServiceTypes()">
            <label class="tab_title col-sm-5" for="ServiceType">Ստացման եղանակ</label>
            <div class="col-sm-6">
                <div class="row">
                    <select ng-show="serviceTypes" ng-disabled="order.Type == 91 || order.Type == 92 || order.Type == 94" data-ng-model="order.PensionApplication.ServiceType" name="ServiceType" ng-change="forArcaPensionCard=undefined;order.PensionApplication.Account=undefined;order.PensionApplication.CardType=undefined;accounts=undefined;(order.PensionApplication.ServiceType==3 || order.PensionApplication.ServiceType==5)?getAccountsForOrder(order.PensionApplication.ServiceType):''" id="ServiceType" class="form-control" required
                            data-ng-options="k as v for (k,v) in serviceTypes">
                        <option value="" hidden="hidden" style="margin-left:25px">Ընտրեք...</option>
                    </select>
                </div>
                <div class="row">
                    <ng-messages ng-if="PensionApplicationOrderForm.$submitted || PensionApplicationOrderForm.ServiceType.$dirty"
                                 for="PensionApplicationOrderForm.ServiceType.$error">
                        <ng-message when="required" class="errors">
                            Ծառայության տեսակը ընտրված չէ
                        </ng-message>
                    </ng-messages>
                </div>
            </div>
        </div>

        <div class="form-group" ng-if="order.PensionApplication.ServiceType==1 && order.Type != 91">
            <label class="tab_title col-sm-5" for="forArcaPensionCard">Ընտրել ARCA PENSION քարտ</label>
            <div class="col-sm-6">
                <div class="row">
                    <input type="checkbox" ng-disabled="order.Type == 92 || order.Type == 94" class="checkbox-inline" id="forArcaPensionCard" ng-model="$parent.forArcaPensionCard" ng-click="$parent.forArcaPensionCard==true?getAccountsForOrder(order.PensionApplication.ServiceType):''" ng-true-value="true" ng-false-value="false">
                </div>

            </div>
        </div>

        
        <div ng-if="order.PensionApplication.ServiceType==3 || order.PensionApplication.ServiceType==5 || order.PensionApplication.ServiceType==6 || forArcaPensionCard || order.Type == 91  || order.Type == 94">
            <div ng-init="((order.Type == 91 || order.Type == 94) &&  order.PensionApplication.ServiceType==1)?getAccountsForOrder(order.PensionApplication.ServiceType):''"></div>
            <div ng-init="((order.Type == 91 || order.Type == 94) && order.PensionApplication.ServiceType==4)?getAccountsForOrder(5):''"></div>
            <div ng-init="((order.Type == 91 || order.Type == 94) &&  order.PensionApplication.ServiceType==2)?getAccountsForOrder(3):''"></div>
            <div ng-init="((order.Type == 91 || order.Type == 94) &&  order.PensionApplication.ServiceType==3)?getAccountsForOrder(3):''"></div>
            <div ng-init="((order.Type == 91 || order.Type == 94) &&  order.PensionApplication.ServiceType==5)?getAccountsForOrder(order.PensionApplication.ServiceType):''"></div>
            <div ng-init="(order.PensionApplication.ServiceType==6)?getAccountsForOrder(order.PensionApplication.ServiceType):''"></div>
            <paymentaccount accounts="accounts"
                            accountselectname="account"
                            accountnumber="order.Type == 94?order.PensionApplication.Account.AccountNumber:undefined"
                            account="order.PensionApplication.Account"
                            check="0"
                            nameforaccount="'Հաշվեհամար'"
                            nameforcurrency="''"
                            disable="order.Type == 94 && order.PensionApplication.ServiceType!=4 && order.PensionApplication.ServiceType!=5"
                            labels-required>
            </paymentaccount>
            <div class="row">
                <div class="col-sm-6"></div>
                <ng-messages ng-if="PensionApplicationOrderForm.$submitted || PensionApplicationOrderForm.account.$dirty"
                             for="PensionApplicationOrderForm.account.$error">
                    <ng-message when="required" class="errors">
                        Հաշիվը ընտրված չէ
                    </ng-message>
                </ng-messages>
            </div>
        </div>


        <div class="form-group" ng-init="getCardsType()" ng-if="order.PensionApplication.ServiceType==2">
            <label class="tab_title col-sm-5" for="CardType">Քարտի տեսակ</label>
            <div class="col-sm-6">
                <div class="row">
                    <select ng-show="serviceTypes" ng-disabled="order.Type == 91 || order.Type == 92" data-ng-model="order.PensionApplication.CardType" name="CardType" id="CardType" class="form-control" required
                            data-ng-options="k as v for (k,v) in cardsType">
                        <option value="" hidden="hidden" style="margin-left:25px">Ընտրեք...</option>
                    </select>
                </div>
                <div class="row">
                    <ng-messages ng-if="PensionApplicationOrderForm.$submitted || PensionApplicationOrderForm.ServiceType.$dirty"
                                 for="PensionApplicationOrderForm.ServiceType.$error">
                        <ng-message when="required" class="errors">
                            Քարտի տեսակը ընտրված չէ
                        </ng-message>
                    </ng-messages>
                </div>
            </div>
        </div>


        <div class=" form-group">
            <label class="tab_title col-sm-5 " for="ContractDate">Պայմանագրի սկիզբ</label>
            <div class="col-sm-6 row">
                <input type="date" class="form-control tab_title" disabled="disabled" style="text-align: left;" id="ContractDate" name="ContractDate" ng-model="order.PensionApplication.ContractDate">
            </div>
        </div>
        <div class=" form-group">
            <label class="tab_title col-sm-5 " for="DateOfNormalEnd">Պայմանագրի վերջ</label>
            <div class="col-sm-6 row">
                <input type="date" class="form-control tab_title " disabled="disabled" style="text-align: left;" id="DateOfNormalEnd" name="DateOfNormalEnd" ng-model="order.PensionApplication.DateOfNormalEnd">
            </div>
        </div>
    </div>

</form>











