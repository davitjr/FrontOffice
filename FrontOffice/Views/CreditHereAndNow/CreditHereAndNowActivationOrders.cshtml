@{
    Layout = null;
}
<style type="text/css">
    #CreditHNActivationActivationOrderForm .ui-widget input {
        font-family: 'webly_semibold';
    }

</style>
<div id="creditHereAndNowLoad" class="overlay hidden">
    <img class="loader-image non-selectable" src="../../Content/Images/invisible.png" />
</div>
<form ng-controller="CreditHereAndNowActivationOrdersCtrl" name="CreditHNActivationActivationOrderForm" style="min-width:700px" data-ng-init="getIncompletePreOrdersCount()" id="CreditHNActivationActivationOrderForm" role="form" class="form-horizontal" prevent-enter-submit novalidate labels-required>
    <div id="receivecheque_menu" class="menu-bar dialog-menu light-green">
        <ul id="menu" class="dialog-menu">
            <li class="root-menu-item-4 " ng-click="CreditHNActivationActivationOrderForm.$setSubmitted();CreditHNActivationActivationOrderForm.$valid && saveCreditHereAndNowActivationOrders();">
                <span id="h-menu-item-4"><img id="h-menu-icon-4" class="submitButton h-menu-icon">Պահպանել</span>
            </li>
            <li class="root-menu-item-5 " ng-click="closeDialog()">
                <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon cancelButton">Հրաժարվել</span>
            </li>
            <li class="root-menu-item-5 " ng-click="resetIncompletePreOrderDetailQuality()">
                <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon applicationButton">Հեռացնել նախորդ խմբաքանակի չձևավորված հայտերը</span>
            </li>
        </ul>
        <div id="right-menu" style="display:none;">
        </div>
    </div>
    <div class="col-sm-12" style="padding-left:10px">
        <div style="max-width:630px">
            <errors></errors>
        </div>
        <div class="form-group">
            <label class="tab_title col-sm-7" for="RegistrationDate">Մուտքագրման ա/թ</label>
            <div class="col-sm-5 row ">
                <div class="row multiple_columns col-sm-8">
                    <input type="date" disabled="disabled" class="form-control tab_title unstyled" id="RegistrationDate" name="RegistrationDate" ng-model="order.RegistrationDate" style="text-align:left;" required>
                </div>
            </div>
        </div>
        <div class="form-group">
            <label class="tab_title col-sm-7" for="credits.length">Ակտիվացման ենթակա վարկերի քանակ`</label>
            <div class="col-sm-5 row ">
                <div class="row multiple_columns col-sm-8">
                    <label >{{credits.length}}</label>

                </div>
            </div>
        </div>
                <div class="form-group" >
            <label class="tab_title col-sm-7" for="incompletePreOrdersCount" style="color:#A94442">Նախորդ խմբաքանակից դեռևս չձևավորված հայտերի քանակ`</label>
            <div class="col-sm-5 row ">
                <div class="row multiple_columns col-sm-8" style="color:#A94442">
                    <label id="incompletePreOrdersCount">{{incompletePreOrdersCount}}</label>
                </div>
            </div>
        </div>
        <br />
    </div>
</form>
