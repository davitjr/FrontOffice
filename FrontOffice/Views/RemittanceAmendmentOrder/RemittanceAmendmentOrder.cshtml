@{
    Layout = null;
}


<div id="remittanceAmendmentOrderLoad" class="overlay hidden">
    <img class="loader-image non-selectable" src="../../Content/Images/invisible.png" />
</div>
<form ng-controller="RemittanceAmendmentOrderCtrl" ng-init="initRemittanceAmendmentOrder()" name="RemittanceAmendmentOrderForm" style="min-width:550px" id="RemittanceAmendmentOrderForm" role="form" class="form-horizontal" prevent-enter-submit novalidate labels-required>
    <div id="receivecheque_menu" class="menu-bar dialog-menu light-green" ng-if="canChangeRemittance">
        <ul id="menu" class="dialog-menu">
            <li class="root-menu-item-4 " ng-if="canChangeRemittance" ng-click="RemittanceAmendmentOrderForm.$setSubmitted();RemittanceAmendmentOrderForm.$valid && saveRemittanceAmendmentOrder();">
                <span id="h-menu-item-4"><img id="h-menu-icon-4" class="submitButton h-menu-icon">Պահպանել</span>
            </li>
            <li ng-if="canChangeRemittance" class="root-menu-item-4 " ng-click="getRemittanceAmendmentApplication(1);">
                <span id="h-menu-item-4"><img id="h-menu-icon-4" class="h-menu-icon printButton">Տվյալների ուղղման դիմում</span>
            </li>
            <li class="root-menu-item-5 " onclick="CloseBPDialog('newRemittanceAmendmentOrder')">
                <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon cancelButton">Հրաժարվել</span>
            </li>
        </ul>
        <div id="right-menu" style="display:none;">
        </div>
    </div>
    <div class="col-sm-12" style="padding-left:10px">
        <br />
        <div>
            <errors></errors>
        </div>
        <div class="form-group">
            <label class="tab_title col-sm-5" for="RegistrationDate">Մուտքագրման ա/թ</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="date" disabled="disabled" class="form-control tab_title unstyled" id="RegistrationDate" name="RegistrationDate" ng-model="order.RegistrationDate" style="text-align:left;" required>
                </div>

            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="URN">Փոխանցման հսկիչ համար<span class="redStyle">*</span></label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" disabled="disabled" class="form-control tab_title unstyled" id="URN" name="URN" ng-model="order.URN" style="text-align:left;" required>
                </div>

            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="AmendmentReasonCode">Փոփոխման պատճառ<span class="redStyle">*</span></label>

            <div class="col-sm-7">
                <div class="row">

                    <div>
                        <select ng-show="amendmentReasons" data-ng-model="order.AmendmentReasonCode" class="form-control"
                                data-ng-options="k as v for (k,v) in amendmentReasons" name="AmendmentReasonCode" required>
                            <option value="" style="margin-left:25px">-Ընտրեք-</option>
                        </select>
                    </div>
                </div>
                <div class="row">
                    <ng-messages ng-if="RemittanceAmendmentOrderForm.$submitted"
                                 for="RemittanceAmendmentOrderForm.AmendmentReasonCode.$error">
                        <ng-message when="required" class="errors">
                            Փոփոխման պատճառը մուտքագրված չէ
                        </ng-message>

                    </ng-messages>
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="BeforeBeneLastName">Ստացողի ազգանուն</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" disabled="disabled" class="form-control tab_title unstyled" id="BeforeBeneLastName" name="BeforeBeneLastName" ng-model="order.BeforeBeneLastName" style="text-align:left;">
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="BeforeBeneFirstName">Ստացողի անուն</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" disabled="disabled" class="form-control tab_title unstyled" id="BeforeBeneFirstName" name="BeforeBeneFirstName" ng-model="order.BeforeBeneFirstName" style="text-align:left;">
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="BeforeBeneMiddleName">Ստացողի հայրանուն</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" disabled="disabled" class="form-control tab_title unstyled" id="BeforeBeneMiddleName" name="BeforeBeneMiddleName" ng-model="order.BeforeBeneMiddleName" style="text-align:left;">
                </div>
            </div>
        </div>


        <div class="form-group">
            <label class="tab_title col-sm-5" for="BeneficiaryLastName">Ստացողի ազգանուն (նոր)</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" class="form-control tab_title unstyled" id="BeneficiaryLastName" name="BeneficiaryLastName" ng-model="order.BeneficiaryLastName" style="text-align:left;">
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="BeneficiaryFirstName">Ստացողի անուն (նոր)</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" class="form-control tab_title unstyled" id="BeneficiaryFirstName" name="BeneficiaryFirstName" ng-model="order.BeneficiaryFirstName" style="text-align:left;">
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="BeneficiaryMiddleName">Ստացողի հայրանուն (նոր)</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" class="form-control tab_title unstyled" id="BeneficiaryMiddleName" name="BeneficiaryMiddleName" ng-model="order.BeneficiaryMiddleName" style="text-align:left;">
                </div>
            </div>
        </div>


        <div class="form-group">
            <label class="tab_title col-sm-5" for="BeforeNATBeneficiaryLastName">Ստացողի ազգանուն (Հայերեն)</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" disabled="disabled" class="form-control tab_title unstyled" id="BeforeNATBeneficiaryLastName" name="NATBeneficiaryLastName" ng-model="order.BeforeNATBeneficiaryLastName" style="text-align:left;">
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="BeforeNATBeneficiaryFirstName">Ստացողի անուն (Հայերեն)</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" disabled="disabled" class="form-control tab_title unstyled" id="BeforeNATBeneficiaryFirstName" name="BeforeNATBeneficiaryFirstName" ng-model="order.BeforeNATBeneficiaryFirstName" style="text-align:left;">
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="BeforeNATBeneficiaryMiddleName">Ստացողի հայրանուն (Հայերեն)</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" disabled="disabled" class="form-control tab_title unstyled" id="BeforeNATBeneficiaryMiddleName" name="BeforeNATBeneficiaryMiddleName" ng-model="order.BeforeNATBeneficiaryMiddleName" style="text-align:left;">
                </div>
            </div>
        </div>


        <div class="form-group">
            <label class="tab_title col-sm-5" for="NATBeneficiaryLastName">Ստացողի ազգանուն (Հայերեն) (նոր)</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" class="form-control tab_title unstyled" id="NATBeneficiaryLastName" name="NATBeneficiaryLastName" ng-model="order.NATBeneficiaryLastName" style="text-align:left;">
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="NATBeneficiaryFirstName">Ստացողի անուն (Հայերեն) (նոր)</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" class="form-control tab_title unstyled" id="NATBeneficiaryFirstName" name="NATBeneficiaryFirstName" ng-model="order.NATBeneficiaryFirstName" style="text-align:left;">
                </div>
            </div>
        </div>

        <div class="form-group">
            <label class="tab_title col-sm-5" for="NATBeneficiaryMiddleName">Ստացողի հայրանուն (Հայերեն) (նոր)</label>
            <div class="col-sm-7 ">
                <div class="row">
                    <input type="text" class="form-control tab_title unstyled" id="NATBeneficiaryMiddleName" name="NATBeneficiaryMiddleName" ng-model="order.NATBeneficiaryMiddleName" style="text-align:left;">
                </div>
            </div>
        </div>
    </div>


</form>

