@{
    Layout = null;
}


<style>

    #loan_details.bhoechie-tab-content {
        background-color: #f2f2f2 !important
    }
</style>


<head>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
</head>

<div ng-controller="LeasingCtrl" data-ng-init="getLeasing(productId);" id="leasingdetails">
    <div id="leasingdetails_menu" class="menu-bar dialog-menu light-green details-menu">
        <ul id="menu" class="dialog-menu">
            <li class="root-menu-item-4 ">
                <span id="h-menu-item-4" onclick="return false;">
                    <img id="h-menu-icon-4" class="h-menu-icon applicationButton">Լիզինգի գրաֆիկ
                    <img id="arrow-down" class="arrowButton">
                </span>
                <ul>
                    <li id="v-menuItem-3.1" class="" ng-if="leasing.Quality!=10" ng-controller="PopUpCtrl" ng-click="openWindow('/Leasing/LeasingGrafik','Լիզինգի գրաֆիկ','leasinggrafik')" data-permission-selector="">
                        <span>Դիտել</span>
                    </li>
                    <li id="v-menuItem-3.2" ng-if="leasing.Quality!=10" ng-click="getLeasingGrafikApplication()" data-permission-selector="">
                        <span>Տպել</span>
                    </li>
                    <li id="v-menuItem-3.2" ng-controller="PopUpCtrl" ng-click="openWindow('/Leasing/LeasingGrafikFirst','Լիզինգի սկզբնական գրաֆիկ','leasinggrafik')" data-permission-selector="">
                        <span>Սկզբնական</span>
                    </li>
                </ul>
            </li>
            <li class="root-menu-item-3 " ng-class="{'my_disable':disabelButton==true}" ng-if="isClosed!=true && leasing.Quality!=undefined" ng-controller="PopUpCtrl" ng-click="openWindow('/Leasing/LeasingPaymentOrder','Լիզինգի մարում','leasingPayment')">
                <span id="h-menu-item-3"><img id="h-menu-icon-3" class="cashInButton h-menu-icon">Լիզինգի մարում</span>
            </li>

            <li class="root-menu-item-3 " ng-class="{'my_disable':disabelButton==true}" ng-if="isClosed!=true && leasing.Quality!=undefined" ng-controller="PopUpCtrl" ng-click="openWindow('/Leasing/LeasingTransitPaymentOrder','Լիզինգի մարում (կանխիկ)','leasingTransitPayment')">
                <span id="h-menu-item-3"><img id="h-menu-icon-3" class="cashInButton h-menu-icon">Լիզինգի մարում (կանխիկ)</span>
            </li>

            <li class="root-menu-item-4 " ng-if="loan.Quality!=10">
                <span id="h-menu-item-4" onclick="return false;">
                    <img id="h-menu-icon-4" class="h-menu-icon applicationButton">Այլ
                    <img id="arrow-down" class="arrowButton">
                </span>
                <ul>
                    <li id="v-menuItem-3.1" class="" ng-controller="PopUpCtrl" ng-click="params={productId:leasing.ProductId};openWindow('/Leasing/LeasingOverdueDetails','Ժ/անց պատմություն')" data-permission-selector="">
                        <span>Ժ/անց պատմություն</span>
                    </li>
                </ul>
            </li>
        </ul>
        <div id="right-menu" style="display:none;">
        </div>
    </div>
    <section class="panel panel-info" style="max-width:800px;margin-left:10px">
        <header class="panel-heading">
            <span class="panel-title" style="font-size: 14px;"><span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span></span>
        </header>
        <div class="panel-body">            
            <div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable redStyle" ng-if="leasing.InsuranceAmount==0">Հերթական մարում</div>
                <div class="product-detail-cell product-detail-lable redStyle" ng-if="leasing.InsuranceAmount>0">Հերթական մարում և ապահովագրավճար</div>                
                <div class="product-detail-cell product-detail-value redStyle" style="font-weight:bold;" ng-if="finalAmount = (leaisng.PaymentsCount==1 ? leasing.LeasingPayment+leasing.InsuranceAmount+leasing.PrepaymentAmount+leasing.FeeAmount+leaisng.FinalFee+leasing.PenaltyRate:leasing.LeasingPayment+leasing.InsuranceAmount+leasing.PrepaymentAmount+leasing.FeeAmount+leasing.PenaltyRate)">{{finalAmount | number:2}} AMD</div>
            </div>
            
            <div class="product-detail-row" ng-if="leasing.Quality==5">
                <div class="product-detail-cell product-detail-lable redStyle" style="color:red">Ժամկետանց մաս</div>
                <div class="product-detail-cell product-detail-value redStyle" style="font-weight:bold;">{{ ::leasing.SumOverdueCapitalsString }} AMD</div>
            </div>

        </div>
    </section>
