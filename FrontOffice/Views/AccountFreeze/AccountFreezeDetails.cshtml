<form style="width:500px" ng-controller="AccountFreezeCtrl" ng-init="getAccountFreezeDetails(freezeid)">
    <loading-control style="margin-left:20em" scopevariable="freezeDetails"></loading-control>
    <div ng-show="freezeDetails" class="product-details bhoechie-tab-content" style="padding:30px;padding-top:10px;">
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Պատճառ</div>
            <div class="product-detail-cell product-detail-value">{{freezeDetails.ReasonDescription}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Մուտքագրման ա/թ</div>
            <div class="product-detail-cell product-detail-value">{{freezeDetails.RegistrationDate| mydate | date: 'dd/MM/yyyy hh:mm'}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Սառեցման ա/թ` ըստ գրության</div>
            <div class="product-detail-cell product-detail-value">{{(freezeDetails.FreezeDateByDocument==='')? '-': freezeDetails.FreezeDateByDocumnet| mydate | date: 'dd/MM/yyyy'}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Հաշվի սառեցման ա/թ</div>
            <div class="product-detail-cell product-detail-value">{{(freezeDetails.FreezeDate==='')? '-': freezeDetails.FreezeDate| mydate | date: 'dd/MM/yyyy'}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գումարի սառեցման ա/թ</div>
            <div class="product-detail-cell product-detail-value">{{(freezeDetails.AmountFreezeDate==='')? '-': freezeDetails.AmountFreezeDate| mydate | date: 'dd/MM/yyyy'}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Սառեցված գումար</div>
            <div class="product-detail-cell product-detail-value">{{freezeDetails.FreezeAmount|number:2}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Սառեցնող Պ/Կ</div>
            <div class="product-detail-cell product-detail-value">{{(freezeDetails.FreezeUserId===0)? '-': freezeDetails.FreezeUserId}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Նկարագրություն</div>
            <div class="product-detail-cell product-detail-value">{{(freezeDetails.FreezeDescription==='')? '-': freezeDetails.FreezeDescription}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Ապասառեցման ա/թ</div>
            <div class="product-detail-cell product-detail-value">{{(freezeDetails.UnfreezeDate==='')? '-': freezeDetails.UnfreezeDate| mydate | date: 'dd/MM/yyyy'}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Ապասառեցման պատճառ</div>
            <div class="product-detail-cell product-detail-value">{{freezeDetails.UnfreezeReasonDescription}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Ապասառեցնող Պ/Կ</div>
            <div class="product-detail-cell product-detail-value">{{(freezeDetails.UnfreezeUserId===0)? '-': freezeDetails.UnfreezeUserId}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Նկարագրություն</div>
            <div class="product-detail-cell product-detail-value">{{(freezeDetails.UnfreezeDescription==='')? '-': freezeDetails.UnfreezeDescription}}</div>
        </div>
    </div>
</form>
