
@{
    Layout = null;
}

<form ng-show="false">
    <div class="menu-bar dialog-menu light-green">
    </div>
    <div>
    </div>
</form>
<form style="min-width:500px">
    <div class="product-details bhoechie-tab-content" style="padding:20px;">
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Բալանսային հաշիվ</div>
            <div class="product-detail-cell product-detail-value">{{provision.OutBalance}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գումար</div>
            <div class="product-detail-cell product-detail-value">{{provision.Amount | number:2 }} {{provision.Currency}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Տեսակ</div>
            <div class="product-detail-cell product-detail-value">{{provision.TypeDescription}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Նկարագրություն</div>
            <div class="product-detail-cell product-detail-value">{{provision.Description}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Ծրագիր</div>
            <div class="product-detail-cell product-detail-value">{{provision.LoanProgram}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Պարկի համար</div>
            <div class="product-detail-cell product-detail-value">{{provision.PacketNumber}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գրավի վկայականի համար</div>
            <div class="product-detail-cell product-detail-value">{{provision.LicenceNumber}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Անշարժ գույքի. սեփ. վկայականի համար</div>
            <div class="product-detail-cell product-detail-value">{{provision.PropertyLicenceNumber}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գրավի կցման ա/թ</div>
            <div ng-if="provision.ActivateDate!=null" class="product-detail-cell product-detail-value">{{provision.ActivateDate | mydate | date: 'dd/MM/yyyy'}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գրավի փակման ա/թ</div>
            <div ng-if="provision.ClosingDate!=null" class="product-detail-cell product-detail-value">{{provision.ClosingDate | mydate | date: 'dd/MM/yyyy'}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գրավի ներքին N</div>
            <div class="product-detail-cell product-detail-value">{{provision.Id}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գրավի կցման ՊԿ</div>
            <div class="product-detail-cell product-detail-value">{{provision.ActivatedSetNumber}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գրավի փակման ՊԿ</div>
            <div ng-if="provision.MaturedSetNumber!=0" class="product-detail-cell product-detail-value">{{provision.MaturedSetNumber}}</div>
        </div>
        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գրավի փակման հիմք</div>
            <div class="product-detail-cell product-detail-value">{{provision.ClosingReason}}</div>
        </div>
    </div>
</form>