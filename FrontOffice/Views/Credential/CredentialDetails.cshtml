<form ng-controller="CredentialCtrl" style="width:600px;">
    <div class="product-details bhoechie-tab-content" style="min-width:600px">

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Լիազորված անձ</div>
            <div class="product-detail-cell product-detail-value">{{credential.AssigneeList[0].AssigneeFirstName}} {{credential.AssigneeList[0].AssigneeLastName}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Լիազորագրի վավերացման ամսաթիվ</div>
            <div class="product-detail-cell product-detail-value">{{credential.CredentialGivenDate | mydate | date: 'dd/MM/yyyy'}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Նոտար</div>
            <div class="product-detail-cell product-detail-value">{{credential.Notary}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Նոտարական տարածք</div>
            <div class="product-detail-cell product-detail-value">{{credential.NotaryTerritory}}</div>
        </div>

        @*<div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Մուտքագրման ամսաթիվ</div>
                <div class="product-detail-cell product-detail-value">{{credential.EstimateAmount | number:2}} {{collateralDetails.Currency}}</div>
            </div>*@

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Սեղանամատյանի համար</div>
            <div class="product-detail-cell product-detail-value">{{credential.LedgerNumber}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Թարգմանության վավերացման ամսաթիվ</div>
            <div class="product-detail-cell product-detail-value">{{credential.TranslationValidationDate  | mydate | date: 'dd/MM/yyyy'}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Թարգմանությունը վավերացրած նոտարի նոտարական տարածք</div>
            <div class="product-detail-cell product-detail-value">{{credential.TranslationOfNotaryTerritory}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Թարգմանությունը վավերացրած նոտար</div>
            <div class="product-detail-cell product-detail-value">{{credential.TranslationOfNotary}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Թարգմանության վավերացման սեղանամատյանի համար</div>
            <div class="product-detail-cell product-detail-value">{{credential.TranslationValidationLedgerNumber}}</div>
        </div>

    </div>
</form>