



<form ng-controller="CashBookCtrl" ng-init="getCashBookOrder(selectedOrderId)" style="width:600px;">
    <loading-control style="margin-left:20em" scopevariable="order"></loading-control>
    <div ng-if="order.Quality!=undefined && order.Quality !=31 && order.Quality !=30 && order.Quality !=32 && order.Quality !=40 && order.Quality !=41 && order.Quality !=6 && order.Quality !=50" id="personalDepositOrderDetails_menu" class="menu-bar dialog-menu light-green " style="margin-left:-5px;margin-top:0">
        <ul id="menu" class="dialog-menu">

            <li class="root-menu-item-5 " ng-if="order.Quality !=31 && order.Quality !=30 && order.Quality !=32 && order.Quality !=40 && order.Quality !=41 && order.Quality !=6 && order.Quality !=50" ng-controller="PopUpCtrl" ng-click="params={removableorder:order};openWindow('/RemovalOrder/RemovalOrder','Հրաժարման հայտ','removalOrder',callbackgetDepositOrder)">
                <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon closeAccountButton">Հրաժարման հայտ</span>
            </li>

        </ul>
    </div>

	<div class="product-details bhoechie-tab-content" style="min-width:600px;padding-left: 15px;">

		<div class="product-detail-row">
			<div class="product-detail-cell product-detail-lable" style="margin-left:10px">Հայտի համար</div>
			<div class="product-detail-cell product-detail-value">{{order.Id}}</div>
		</div>

		<div class="product-detail-row">
			<div class="product-detail-cell product-detail-lable" style="margin-left:10px">Մուտքագրման ա/թ</div>
			<div class="product-detail-cell product-detail-value">{{order.RegistrationDateString}}</div>
		</div>

		<div class="product-detail-row">
			<div class="product-detail-cell product-detail-lable" style="margin-left:10px">Գործառնական օր</div>
			<div class="product-detail-cell product-detail-value">{{order.OperationDateString}}</div>
		</div>
		<div class="product-detail-row">
			<div class="product-detail-cell product-detail-lable" style="margin-left:10px">Գումար</div>
			<div class="product-detail-cell product-detail-value">{{order.CashBooks[0].Amount}}</div>
		</div>
		<div class="product-detail-row">
			<div class="product-detail-cell product-detail-lable" style="margin-left:10px">Դեբետային հաշիվ</div>
			<div class="product-detail-cell product-detail-value">{{order.CashBooks[0].DebetAccount}}</div>
		</div>
		<div class="product-detail-row">
			<div class="product-detail-cell product-detail-lable" style="margin-left:10px">Կրեդիտային հաշիվ</div>
			<div class="product-detail-cell product-detail-value">{{order.CashBooks[0].CreditAccount}}</div>
		</div>
		<div class="product-detail-row">
			<div class="product-detail-cell product-detail-lable" style="margin-left:10px">Կարգավիճակ</div>
			<div ng-if="order.Quality==30" class="product-detail-cell product-detail-value greenStyle">{{order.QualityDescription}}</div>
			<div ng-if="order.Quality==31" class="product-detail-cell product-detail-value redStyle">{{order.QualityDescription}}</div>
			<div ng-if="order.Quality!=31 && order.Quality!=30" class="product-detail-cell product-detail-value">{{order.QualityDescription}}</div>
		</div>

		<div ng-if="order.Quality==31" class="">
			<div ng-controller="OrderCtrl" ng-init="getOrderRejectHistory(selectedOrderId)" class="" style="clear:both;color:#6F6A6A;font-weight:bold">Մերժման պատճառ`</div>
			<div class="redStyle">&nbsp;&nbsp;&nbsp;{{ rejectHistory.ReasonDescription }}</div>
		</div>
		<div style="clear:both;color:#6F6A6A;font-weight:bold;margin-left:10px">Կատարվել է {{selectedSourceDescription}}-ի միջոցով</div>
		<div class="product-container" style="width:550px">
			<div class="header-title">
				&nbsp;
				<div class="product-collapse tipB fa fa-chevron-down" data-original-title="Թաքցնել" onclick="CollapseProductContainer(this)"></div>
			</div>
			<div class="product-collapsable-section">
				<div class="product-container-header" style="height:44px;">
					<div style="text-align:center" class="col-sm-3">ՊԿ</div>
					<div style="text-align:center" class="col-sm-3">Թղթակից ՊԿ</div>
					<div style="text-align:center" class="col-sm-2">Գումար</div>
					<div style="text-align:center" class="col-sm-4">Լրացուցիչ</div>
				</div>
				<div class="product-content-table-full_height custom-scroll" style="min-height:100px;float:left;">
					<table class="table table-condensed table-hover" style="background-color:white; margin-top:0px !important;">
						<tbody>
							<tr ng-repeat="cashBook in order.CashBooks">
								<td class="col-sm-3" style="text-align:center">{{::cashBook. RegisteredUserID}}<span ng-if="cashBook. RegisteredUserID!=0" style="color: #337AB7; cursor:pointer;" ng-controller="PopUpCtrl" ng-click="params={setNumber:cashBook.RegisteredUserID}; openWindow('/Home/UserDetails','Օգտագործողի  տվյալներ','userdetails')" class="glyphicon glyphicon-info-sign"></span></td>
								<td class="col-sm-3" style="text-align:center">{{::cashBook.CorrespondentSetNumber}}<span ng-if="cashBook.CorrespondentSetNumber!=0" style="color: #337AB7; cursor:pointer;" ng-controller="PopUpCtrl" ng-click="params={setNumber:cashBook.CorrespondentSetNumber}; openWindow('/Home/UserDetails','Օգտագործողի  տվյալներ','userdetails')" class="glyphicon glyphicon-info-sign"></span></td>
								<td class="col-sm-2" style="text-align:center">{{::cashBook.Amount}} {{::cashBook.Currency}}</td>
								<td class="col-sm-4" style="text-align:center">{{::cashBook.Description}}</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>

		</div>
		<div style="clear:both;color:#6F6A6A;font-weight:bold;margin-left:10px">&nbsp;</div>
	</div>
   
</form>

