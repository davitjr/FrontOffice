

@{
    Layout = null;
}
<style type="text/css">
    .chosen-container-multi .chosen-choices {
        border-radius: 4px;
    }

    #TransactionsAutomatedConfirmTimeForm .ui-widget input {
        font-family: 'webly_semibold';
    }

    #TransactionsAutomatedConfirmTimeForm {
        min-width: 400px;
    }

    .maximize-dialog {
        display: none;
    }

    .minimize-dialog {
        display: none;
    }
    .maximize-dialog {
        display: none;
    }

    .minimize-dialog {
        display: none;
    }
</style>
<div id="TransactionsAutomatedConfirmTimeForm" class="overlay hidden">
    <img class="loader-image non-selectable" src="../../Content/Images/invisible.png" />
</div>
<form ng-controller="HBDocumentsCtrl" ng-init="initAutomatedConfirmTime();" name="TransactionsAutomatedConfirmTimeForm" @*ng-submit="confirmRejectClick();"*@ id="TransactionsAutomatedConfirmTimeForm" role="form" novalidate class="form-horizontal" prevent-enter-submit labels-required style="width:400px;">
    <center>
        <div>
            <div class="col-sm-11" style="margin-top:20px;">
                <div class="form-group">
                    <table style="margin-left:15px;">
                        <tr>
                            <td class="tabletd" colspan="4">
                                <div>
                                    <input type="text" class="form-control" name="currentDateTime" id="currentDateTime" title="Ամսաթիվ" disabled ng-model="currentDateTime" style="font-size: 15px;height: 35px;width:150px;font-weight:600" />
                                    <input type="time" class="form-control" name="changeableTime" id="changeableTime" title="Ժամ" ng-model="changeableTime" style="width : 135px; height: 35px;font-size:14px;" />
                                </div>
                            </td>
                        </tr>
                    </table>
                </div>
                <div class="form-group">
                    <div class="col-sm-6 row">
                        <div class="col-md-10 multiple_columns" style="width:400px;margin-left:190px">
                            <button type="button" class="buttonClass" ng-click="changeTime()" style="height:35px">Փոփոխել</button>
                        </div>
                    </div>
                </div>
               
                <br />
            </div>
        </div>
    </center>
</form>












