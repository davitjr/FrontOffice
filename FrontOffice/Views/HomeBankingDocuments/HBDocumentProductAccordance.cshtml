
@{
    Layout = null;
}
<style type="text/css">
    .chosen-container-multi .chosen-choices {
        border-radius: 4px;
    }

    #HBDocumentProductAccordanceForm .ui-widget input {
        font-family: 'webly_semibold';
    }

    #HBDocumentProductAccordanceForm {
        min-width: 600px;
    }

    .redStyle {
        visibility: hidden;
    }

    .maximize-dialog {
        display: none;
    }

</style>
<div id="HBDocumentProductAccordance" class="overlay hidden">
    <img class="loader-image non-selectable" src="../../Content/Images/invisible.png" />
</div>
<form ng-controller="HBDocumentsCtrl" ng-init="initProductAccordanceFields();" name="HBDocumentProductAccordanceForm" id="HBDocumentProductAccordanceForm" role="form" novalidate class="form-horizontal" prevent-enter-submit labels-required style="width:650px;">

    <div>
        <div class="col-sm-11" style="margin-top:20px;">
            <div class="col-sm-12">

                <div class=" form-group">
                    <label class="tab_title col-sm-5 " for="RegistrationDate" ng-model="lblTransfer" ng-show="showLblTransfer" style="font-weight:bold">Փոխանցում</label>
                    <label class="tab_title col-sm-5 " for="RegistrationDate" ng-model="lblConvertation" ng-show="showLblConvertation" style="font-weight:bold">Փոխարկում</label>
                </div>

                <div class=" form-group">
                    <label class="tab_title col-sm-5 " for="RegistrationDate">Գործարքի կոդ</label>
                    <div class="col-sm-6 row">
                        <div class="multiple_columns col-sm-8 ">
                            <input disabled="disabled" type="number" class="form-control tab_title unstyled" style="text-align: left;" id="TransactionCode" name="TransactionCode" ng-model="details.TransactionCode" />
                        </div>
                    </div>
                </div>

                <div class=" form-group">
                    <label class="tab_title col-sm-5 " for="RegistrationDate">Ամսաթիվ</label>
                    <div class="col-sm-6 row">
                        <div class="multiple_columns col-sm-8 ">
                            <input disabled="disabled" type="datetime" class="form-control tab_title unstyled" style="text-align: left;" id="TransactionDate" name="TransactionDate" ng-model="details.TransactionDate">
                        </div>
                    </div>
                </div>

                <div class=" form-group">
                    <label class="tab_title col-sm-5 " for="RegistrationDate">ՊԿ</label>
                    <div class="col-md-6 row" style="margin-left:4px;">
                        <div class="multiple_columns col-md-5">
                            <div style="display:inline-flex;width:140px;margin-left:-20px;">

                                <input type="text" class="form-control" name="setNumber" id="setNumber" placeholder="ՊԿ" ng-model="details.SetNumber" style="text-align: left;" disabled="disabled"/>
                                <span data-placement="left" data-toggle="tooltip" class="input-group-addon fa fa-info-circle" style="color:#337AB7;display: inline-block;padding-right: 19px;padding-top: 10px;" ng-controller="PopUpCtrl" ng-click="params={setNumber:(details.SetNumber)}; openWindow('/Home/UserDetails','Աշխատակցի  տվյալներ','userdetails')"></span>

                            </div>
                        </div>
                    </div>
                </div>
                
                <div class=" form-group">
                    <label class="tab_title col-sm-5 " for="RegistrationDate">Համար</label>
                    <div class="col-sm-6 row">
                        <div class="multiple_columns col-sm-8 ">
                            <input disabled="disabled" type="number" class="form-control tab_title unstyled" style="text-align: right;" id="UniqNumber" name="UniqNumber" ng-model="details.UniqNumber">
                        </div>
                    </div>
                </div>

                <div class=" form-group">
                    <label class="tab_title col-sm-5 " for="RegistrationDate">Ներքին տրանզակցիայի համար</label>
                    <div class="col-sm-6 row">
                        <div class="multiple_columns col-sm-8 ">
                            <input disabled="disabled" type="number" class="form-control tab_title unstyled" style="text-align: right;" id="InternalTransactionCode" name="InternalTransactionCode" ng-model="details.InternalTransactionCode">
                        </div>
                    </div>
                </div>
            </div>

            <br />

            </div>
        </div>
</form>











