            <div class="col-sm-7">
                <input name="descr" type="text" class="form-control tab_title" style="text-align: left" data-ng-model="messagingSession.description" required />
                <ng-messages ng-if="addLeasingStatementSession.$submitted || addLeasingStatementSession.descr.$dirty"
