@{
    Layout = null;
}
<form id="addLeasingStatementSession" ng-controller="LeasingStatementSessionCtrl" ng-init="setDates()" style="min-width:500px;min-height:175px;overflow-x:hidden" name="addLeasingStatementSession" role="form" class="form-horizontal" labels-required>

    <div id="LeasingStatementSessionLoad" class="overlay hidden">
        <img class="loader-image non-selectable" src="../../Content/Images/invisible.png" />
    </div>
    <div id="addLeasingStatementSession_menu" class="menu-bar dialog-menu light-green">
        <ul id="menu" class="dialog-menu">
            <li class="root-menu-item-4 " ng-click="addLeasingStatementSession.$setSubmitted();addLeasingStatementSession.$valid && createLeasingStatementSession()">
                <span id="h-menu-item-4"><img id="h-menu-icon-4" class="submitButton h-menu-icon">Պահպանել</span>
            </li>
            <li class="root-menu-item-5 " onclick="CloseBPDialog('addLeasingStatementSession')">
                <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon cancelButton">Հրաժարվել</span>
            </li>
        </ul>
    </div>
    <div style="max-width:400px;margin-left:5px">
        <errors></errors>
    </div>
    <div class="form-group col-sm-12">
        <label class="tab_title col-sm-4" for="startDate">Սկիզբ </label>
        <div class="col-sm-8">
            <div class="row">
                <input type="date" class="form-control " id="startDate" name="startDate" ng-model="startDate" required>
            </div>
            <div class="row">
                <ng-messages ng-if="addLeasingStatementSession.$submitted || addLeasingStatementSession.startDate.$dirty"
                             for="addLeasingStatementSession.startDate.$error">
                    <ng-message when="required" class="errors">
                        Սկիզբը բացակայում է
                    </ng-message>
                </ng-messages>
            </div>
        </div>
    </div>
    <div class="form-group col-sm-12">
        <label class="tab_title col-sm-4" for="endDate"> Վերջ </label>
        <div class="col-sm-8">
            <div class="row">
                <input type="date" class="form-control " id="endDate" name="endDate" ng-model="endDate" required>
            </div>
            <div class="row">
                <ng-messages ng-if="addLeasingStatementSession.$submitted || addLeasingStatementSession.endDate.$dirty"
                             for="addLeasingStatementSession.endDate.$error">
                    <ng-message when="required" class="errors">
                        Վերջը բացակայում է
                    </ng-message>
                </ng-messages>
            </div>
        </div>
    </div>


