<style type="text/css">
    *[tabindex]:focus {
        outline: 3px green solid !important;
    }
</style>

<form style=" overflow-y: hidden;  overflow-x: hidden;  " name="AddForTable1" ng-controller="LeasingCustomerClassificationCtrl" id="deleteConnectionGroundsForNotClassifyingWithCustomerForm">
     

    <div class="menu-bar dialog-menu light-green">
        <ul id="menu" class="dialog-menu">
            <li  tabindex="4" ng-click="CloseConnectionGroundsForNotClassifyingWithCustomer() " ng-keypress="keyPressDeleteConnectionGroundsForNotClassifyingWithCustomer($event)"  style="padding-right:0">
                <span><img id="h-menu-icon-4" class="submitButton h-menu-icon">Հաստատել</span>
            </li>
            <li  tabindex="5" ng-controller="PopUpCtrl" ng-click="CloseBPDialog('deleteConnectionGroundsForNotClassifyingWithCustomerForm')" ng-keypress="keyPressCloseDeleteConnectionGroundsForNotClassifyingWithCustomer($event)"  style="padding-right:0">
                <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon cancelButton">Հրաժարվել</span>
            </li>

        </ul>
    </div>
     


    <div>
        <div class="row">
            <div class=" col-sm-3">
                <div align="left" class="panel-heading">Փոխկապակցված հաճախորդի համար</div>
            </div>
            <div class=" col-sm-3">
                <div class="input-group">
                    <input tabindex="1"  type="text" style="width:280px" id="group" ng-model="notClassificationGrounds.CustomerNumber" disabled="disabled"/>
                    @*<input type="text" style="width:280px" id="group" ng-model="SelectedCustomerNumberLp" />*@
                </div>
            </div>
        </div>

        <div class="row" style=" padding-bottom:15px">
            <div class="col-sm-3">
                <div align="left" class="panel-heading">Զեկուցագրի համարը </div>
            </div>
            <div class="col-sm-3">
                <div class="input-group">
                    <input  tabindex="2" type="text" style="width:280px" id="group" ng-model="closingDocumentNmber" />
                </div>
            </div>
        </div>

        <div class="row" style=" padding-bottom:15px">
            <div class="col-sm-3">
                <div align="left" class="panel-heading">Զեկուցագրի ա/թ </div>
            </div>
            <div class="col-sm-3">
                <div class="input-group">
                    <input  tabindex="3" type="date" ng-model="closingDate" name="Date" style="height:30px;  width:auto" required>
                </div>
            </div>
        </div>
    </div>

</form>
