            <div class="product-detail-cell product-detail-value">{{orderDetails.TransferAdditionalData.ReceiverLivingPlaceDescription}}</div>
        </div>

        <div class="product-detail-row" ng-if="orderDetails.TransferAdditionalData!=undefined">
            <div class="product-detail-cell product-detail-lable">Գումար ուղարկող</div>
            <div class="product-detail-cell product-detail-value">{{orderDetails.TransferAdditionalData.SenderLivingPlaceDescription}}</div>
        </div>


        <div class="product-detail-row" ng-if="orderDetails.TransferAdditionalData!=undefined">
            <div class="product-detail-cell product-detail-lable">Գումարի բնույթը</div>
            <div class="product-detail-cell product-detail-value">{{orderDetails.TransferAdditionalData.TransferAmountTypeDescription}}</div>
        </div>


        <div class="product-detail-row" ng-if="orderDetails.TransferAdditionalData!=undefined">
            <div class="product-detail-cell product-detail-lable">Գումարի փոխանցման նպատակները</div>
            <div class="product-detail-cell product-detail-value">
                <div ng-repeat="p in orderDetails.TransferAdditionalData.TransferAmountPurposes ">
                    {{p.Description}}-{{p.Amount| number:2}} {{orderDetails.Currency}},<span ng-if="p.PurposeCode==11">նշում-{{p.AddInfo}}</span> {{$last ? '' : ', '}}
                </div>
            </div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Կարգավիճակ</div>
            <div ng-if="orderDetails.Quality==30" class="product-detail-cell product-detail-value greenStyle">{{orderDetails.QualityDescription}}</div>
            <div ng-if="orderDetails.Quality==31" class="product-detail-cell product-detail-value redStyle">{{orderDetails.QualityDescription}}</div>
            <div ng-if="orderDetails.Quality!=31 && orderDetails.Quality!=30" class="product-detail-cell product-detail-value">{{orderDetails.QualityDescription}}</div>
        </div>
    </div>
    <div style="clear:both;color:#6F6A6A;font-weight:bold;margin-left:10px" ng-if="$root.SessionProperties.IsCalledFromHB != true">Կատարվել է {{selectedSourceDescription}}-ի միջոցով</div>
    <br /><br />
    <orderattachments orderid="selectedOrderId"></orderattachments>
</form>
