<form ng-open="{{checkForDebitAccountFastPayment!=0?order.Type = 76 : order.Type =102}}" name="ARUSFastTransferOrderForm" role="form" class="form-horizontal col-sm-12" style="width: 1050px; padding: 0" ng-init="getCustomer(); getSenderData();getCountriesWithA3();" novalidate prevent-enter-submit labels-required>
    <div id="interLoad" class="overlay hidden">
        <img class="loader-image non-selectable" src="../../Content/Images/invisible.png" />
