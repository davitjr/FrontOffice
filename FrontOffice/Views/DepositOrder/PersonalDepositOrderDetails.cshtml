<form ng-controller="DepositOrderCtrl" ng-init="getDepositOrder(selectedOrderId)" style="width:600px;">
    <loading-control style="margin-left:20em" scopevariable="order"></loading-control>
    <div ng-if="order.Quality!=undefined && order.Quality !=31 && order.Quality !=30 && order.Quality !=32 && order.Quality !=40 && order.Quality !=41 && order.Quality !=6 && order.Quality !=50 && $root.SessionProperties.IsCalledFromHB != true" id="personalDepositOrderDetails_menu" class="menu-bar dialog-menu light-green " style="margin-left:-5px;margin-top:0">
        <ul id="menu" class="dialog-menu">
    
            <li class="root-menu-item-5 " ng-if="order.Quality !=31 && order.Quality !=30 && order.Quality !=32 && order.Quality !=40 && order.Quality !=41 && order.Quality !=6 && order.Quality !=50"   ng-controller="PopUpCtrl" ng-click="params={removableorder:order};openWindow('/RemovalOrder/RemovalOrder','Հրաժարման հայտ','removalOrder',callbackgetDepositOrder)">
                <span id="h-menu-item-5"><img id="h-menu-icon-5" class="h-menu-icon closeAccountButton">Հրաժարման հայտ</span>
            </li>

        </ul>
    </div>

    <div class="product-details bhoechie-tab-content" style="min-width:600px">

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Հայտի համար</div>
            <div class="product-detail-cell product-detail-value">{{order.Id}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Մուտքագրման ա/թ</div>
            <div class="product-detail-cell product-detail-value">{{order.RegistrationDateString}}</div>
        </div>

        <div class="product-detail-row" ng-if="$root.SessionProperties.IsCalledFromHB != true">
            <div class="product-detail-cell product-detail-lable">Գործառնական օր</div>
            <div class="product-detail-cell product-detail-value">{{order.OperationDateString}}</div>
        </div>

        <div  class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Ավանդի տեսակ</div>
            <div class="product-detail-cell product-detail-value">{{order.Deposit.DepositTypeDescription}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Ավանդի սկիզբ</div>
            <div class="product-detail-cell product-detail-value">{{order.Deposit.StartDateToString}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Ավանդի վերջ</div>
            <div class="product-detail-cell product-detail-value">{{order.Deposit.EndDateToString}}</div>
        </div>

        <div  class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Ավանդի տոկոսադրույք</div>
            <div class="product-detail-cell product-detail-value">{{(order.Deposit.InterestRate*100).toFixed(2)}}%</div>
        </div>

        <div  class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Գումար</div>
            <div class="product-detail-cell product-detail-value">{{order.AmountString}}</div>
        </div>

        <div  class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Արժույթ</div>
            <div class="product-detail-cell product-detail-value">{{order.Deposit.Currency}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Մայր գումարի հաշվեհամար</div>
            <div class="product-detail-cell product-detail-value">{{order.DebitAccount.AccountNumber}} {{order.DebitAccount.Currency}}</div>
        </div>


        <div  class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Ավտոմատ երկարաձգում</div>
            <div class="product-detail-cell product-detail-value">{{RecontractPossibility}}</div>
        </div>

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Քաղվածքի ստացում</div>
            <div class="product-detail-cell product-detail-value">{{order.Deposit.StatementDeliveryTypeDescription}}</div>
        </div>

        <div ng-if="order.AccountType == 2 || order.AccountType == 3" class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Համատեղ հաճախորդներ</div>
            <div ng-repeat="customer in order.ThirdPersonCustomerNumbers">
                <div class="product-detail-cell product-detail-value">{{customer.key}} {{customer.value}} <span ng-show="!$last">, </span> </div>            
            </div>
        </div>


        <div class="product-detail-row" ng-repeat="depositOption in order.Deposit.DepositOption">
            <div class="product-detail-cell product-detail-lable" ng-if="$index==0 && depositOption.OptionGroup==1">Ավանդի օպցիաներ</div>
            <div class="product-detail-cell product-detail-lable" ng-if="$index>0 && depositOption.OptionGroup==1">&nbsp;</div>
            <div class="product-detail-cell product-detail-value" ng-if="depositOption.OptionGroup==1">{{depositOption.TypeDescription}}( {{(depositOption.Rate*100).toFixed(2)}}% )</div>
        </div>

        <div class="product-detail-row" ng-repeat="depositOption in order.Deposit.DepositOption">
            <div class="product-detail-cell product-detail-lable" ng-if="depositOption.OptionGroup==2">Տոկոսագումարների վճարման եղանակ</div>
            <div class="product-detail-cell product-detail-value" ng-if="depositOption.OptionGroup==2">{{depositOption.TypeDescription}}( {{(depositOption.Rate*100).toFixed(2)}}% )</div>
        </div>


        @*<div ng-repeat="thirdPersons in deposit.ThirdPersonDescription" >
            <div class="product-detail-row">
                <div class="product-detail-cell product-detail-lable">Երրորդ անձ</div>
                <div class="product-detail-cell product-detail-value">{{order.ThirdPersonDescription}}</div>
            </div>
        </div>*@

        <div class="product-detail-row">
            <div class="product-detail-cell product-detail-lable">Կարգավիճակ</div>
            <div ng-if="order.Quality==30" class="product-detail-cell product-detail-value greenStyle">{{order.QualityDescription}}</div>
            <div ng-if="order.Quality==31" class="product-detail-cell product-detail-value redStyle">{{order.QualityDescription}}</div>
            <div ng-if="order.Quality!=31 && order.Quality!=30" class="product-detail-cell product-detail-value">{{order.QualityDescription}}</div>
        </div>

        <div ng-if="order.Quality==31" class="">
            <div ng-controller="OrderCtrl" ng-init="getOrderRejectHistory(selectedOrderId)" class="" style="clear:both;color:#6F6A6A;font-weight:bold">Մերժման պատճառ`</div>
            <div class="redStyle">&nbsp;&nbsp;&nbsp;{{ rejectHistory.ReasonDescription }}</div>
        </div>
    </div>
    <div style="clear:both;color:#6F6A6A;font-weight:bold;margin-left:10px" ng-if="$root.SessionProperties.IsCalledFromHB != true">Կատարվել է {{selectedSourceDescription}}-ի միջոցով</div> 
</form>





